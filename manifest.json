{
  "manifest_version": 3,
  "name": "TabVault",
  "version": "1.0",
  "description": "Made by Efficitech",
  "permissions": ["tabs", "tabGroups", "storage"],
  "action": {
		"default_popup": "popup.html",
		"default_icon": "Logo.png"
